<main>
    <section class="profile-page">
        {{{linkBack}}}
        <div class="profile-page__wrap">
            {{{avatar}}}
            <span class="profile-page__name">{{ user.login }}</span>
            <div class="profile-page__info-wrap">
                {{{profileName}}}
            </div>
            {{#each profileInfo}}

            {{{this}}}
            {{/each}}
            <div class="profile-page__links-wrap">
                {{{linkChangeData}}}
                {{{linkChangePassword}}}
                {{{linkExit}}}
            </div>
        </div>
    </section>
</main>
